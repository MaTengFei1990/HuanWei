                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="集装箱个数"
                    android:layout_marginLeft="10dp"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_containerNumber"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="车辆停放保养场停放车辆数（辆）："
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_maintainNumber"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="供休息工人数："
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_restNumber"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"

                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="车辆停放保养场占地面积（平方米）："
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_maintainArea"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                 />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="日处理能力（吨/日）："
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_mangeAbility"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                 />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="处理方式："
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_manageWay"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                 />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
