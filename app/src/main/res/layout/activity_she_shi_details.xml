<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_car_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.hollysmart.huanwei.CarDetailActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/shenlan">

        <ImageView
            android:id="@+id/iv_fanhui"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:padding="15dp"
            android:src="@mipmap/icon_back_bai"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="设施信息"
            android:textColor="@color/baise"
            android:textSize="@dimen/text_teda" />
        <TextView
            android:id="@+id/tv_home"
            android:layout_marginRight="10dp"
            android:textColor="@color/baise"
            android:layout_centerInParent="true"
            android:textSize="@dimen/text_zhong"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="首页"/>

    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginLeft="10dp"
                    android:text="设施名称："
                    android:textSize="@dimen/text_zhong"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_facilityName"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginLeft="10dp"
                    android:text="所属区县："
                    android:textSize="@dimen/text_zhong"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_areaType"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />

            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:text="投入日期:"
                    android:layout_marginLeft="10dp"
                    android:textSize="@dimen/text_zhong"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/tv_useTime"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="接管日期："
                    android:layout_marginLeft="10dp"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_receivingTime"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="所属街道乡镇："
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_streetTown"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="初建投资（万）："
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_investment"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                 />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="统计分类："
                    android:layout_marginLeft="10dp"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_statistical"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="产权单位："
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_propertyrightsNuit"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="作业单位："
                    android:layout_marginLeft="10dp"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_homeworkUnit"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"                      android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_hui"
                />
            <RelativeLayout
                android:padding="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
